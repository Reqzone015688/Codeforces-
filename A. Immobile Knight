#include <bits/stdc++.h>
#define ll long long 
#define tc testcases
using namespace std;
 
int main()
{
    
    ll tc;
    cin >> tc;
    
    while(tc--){
        int n, m;
        cin >> n >> m;
        
        if(max(n, m) == 3){
            if(min(n, m) == 1){
                cout << "1 1" << endl;
            }
            else{
                cout << "2 2" << endl;
            }
        }
        else {
            cout << "1 1" << endl;
        }
        
    }
    
    return 0;
}
