#include <bits/stdc++.h>
#define ll long long 
#define tc testcases
using namespace std;

int main()
{
    
    ll tc;
    cin >> tc;
    
    while(tc--){
        int n, m;
        cin >> n >> m;
        
        if(n > 3 && m > 3){
            cout << 1 << " " << 1 << endl;
        }
        else if((n == 3 && m <= 3) || (n <= 3 && m == 3)){
            cout << 2 << " " << 2 << endl;
        }
        else if((n == 1 && m > 3) || (n > 3 && m == 1)){
            cout << n << " " << m << endl;
        }
        else {
            cout << n << " " << endl;
        }
        
    }
    
    return 0;
}
